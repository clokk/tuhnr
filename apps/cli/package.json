{
  "name": "cogcommit",
  "version": "0.1.0",
  "description": "Document your AI-assisted development with cognitive commits",
  "main": "dist/index.js",
  "bin": {
    "cogcommit": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run build:studio",
    "build:studio": "vite build --config src/studio/vite.config.ts",
    "dev": "ts-node src/index.ts",
    "dev:studio": "vite --config src/studio/vite.config.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "claude",
    "ai",
    "cognitive-commit",
    "development",
    "documentation"
  ],
  "author": "Connor Leisz",
  "license": "MIT",
  "dependencies": {
    "@cogcommit/types": "workspace:*",
    "@hono/node-server": "^1.0.0",
    "@supabase/supabase-js": "^2.93.3",
    "chokidar": "^3.5.0",
    "commander": "^11.0.0",
    "hono": "^4.0.0",
    "open": "^10.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.0.0",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
